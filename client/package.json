{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.12",
    "autoprefixer": "^9.8.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "tailwindcss": "^1.5.2",
    "tailwindcss-bg-alpha": "^2.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "coverage": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css -w",
    "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5500",
  "devDependencies": {
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "tailwindcss-dark-mode": "^1.1.5"
  }
}
